<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Function</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <switchableControllerActions>
                        <TCEforms>
                            <label>Select function</label>
                            <onChange>reload</onChange>
                            <config>
                                <type>select</type>
                                <itemsProcFunc>PITS\Pitsgooglecse\Utility\Fetchlanguage->switchable_decisions</itemsProcFunc>
                                <items>
                                    <numIndex index="0">
                                        <numIndex index="0">Default</numIndex>
                                        <numIndex index="1">Search->default</numIndex>
                                    </numIndex>
                                    <numIndex index="1">
                                        <numIndex index="0">Search Box</numIndex>
                                        <numIndex index="1">Search->searchbox</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">Results View</numIndex>
                                        <numIndex index="1">Search->results</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </switchableControllerActions>
                    <settings.flexform.api>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:pits_googlecse/Resources/Private/Language/locallang.xlf:tx_googlecse_plugin.api</label>
                            <config>
                                <type>input</type>
                                <cols>5</cols>
                                <rows>5</rows>
                            </config>
                        </TCEforms>
                    </settings.flexform.api>
                    <settings.flexform.lang>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:pits_googlecse/Resources/Private/Language/locallang.xlf:tx_googlecse_plugin.lang</label>
                            <config>
                                <type>select</type>
                                <itemsProcFunc>PITS\Pitsgooglecse\Utility\Fetchlanguage->render_languages</itemsProcFunc>
                            </config>
                        </TCEforms>
                    </settings.flexform.lang>
                    <settings.flexform.autocomplete>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:pits_googlecse/Resources/Private/Language/locallang.xlf:tx_googlecse_plugin.autocomplete</label>
                            <config>
                                <type>check</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">Yes</numIndex>
                                        <numIndex index="1">0</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.flexform.autocomplete>
                    <settings.flexform.imagesearch>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:pits_googlecse/Resources/Private/Language/locallang.xlf:tx_googlecse_plugin.image</label>
                            <config>
                                <type>check</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">Yes</numIndex>
                                        <numIndex index="1">0</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.flexform.imagesearch>
                </el>
            </ROOT>
        </sDEF>
        <theme>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Theme Settings</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.flexform.theme>
                        <TCEforms>
                            <exclude>0</exclude>
                            <label>Pick a theme</label>
                            <config>
                                <type>user</type>
                                <form_type>user</form_type>
                                <noTableWrapping>1</noTableWrapping>
                                <userFunc>PITS\Pitsgooglecse\Utility\Fetchlanguage->render_layouts</userFunc>
                            </config>
                        </TCEforms>
                    </settings.flexform.theme>
                    <settings.flexform.enablethumbs>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:pits_googlecse/Resources/Private/Language/locallang.xlf:tx_googlecse_plugin.thumbs</label>
                            <config>
                                <type>check</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">Yes</numIndex>
                                        <numIndex index="1">0</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.flexform.enablethumbs>
                    <settings.flexform.overlayresults>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:pits_googlecse/Resources/Private/Language/locallang.xlf:tx_googlecse_plugin.overlayresults</label>
                            <config>
                                <type>check</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">Yes</numIndex>
                                        <numIndex index="1">0</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.flexform.overlayresults>
                </el>
            </ROOT>
        </theme>
    </sheets>
</T3DataStructure>
